{
    "compilerOptions": {
      "lib": [
        "es2016",
        "es2017",
        "dom"
      ],
      "baseUrl": ".",
      "paths": { "*": ["types/*"] },
        "strict": true,
        "jsx": "react",
        "module": "es6",
        "target": "es5",
        "allowJs": false,
        "sourceMap": true,
        "outDir": "./dist/",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
    },
    "include": [
        "./src/**/*"
    ]
}